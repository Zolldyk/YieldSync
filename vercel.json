{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/app.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://cdnjs.cloudflare.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' https://test-rpc.primordial.bdagscan.com https://primordial.bdagscan.com;"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "BUILD_TIMESTAMP": "1752916945"
  }
}